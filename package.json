{
  "name": "vue-read-more3",
  "version": "1.2.2",
  "description": "a vue.js read-more plugin",
  "main": "dist/index.js",
  "scripts": {
    "build:base": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "build:ssr": "cross-env NODE_ENV=production webpack --config webpack.config.ssr.js --progress --hide-modules",
    "build": "npm run build:base && npm run build:ssr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webcoderkz/read-more.git"
  },
  "author": "dev@webcoder.kz",
  "license": "MIT",
  "homepage": "https://github.com/webcoderkz/read-more.git",
  "bugs": {
    "url": "https://github.com/webcoderkz/read-more/issues"
  },
  "keywords": [
    "read",
    "more",
    "read",
    "less",
    "view",
    "more",
    "vue.js",
    "vue"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@vue/component-compiler-utils": "^1.0.0",
    "babel-loader": "^8.2.2",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^3.12.0",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "cross-env": "^5.1.4",
    "webpack-merge": "^4.1.2"
  }
}
